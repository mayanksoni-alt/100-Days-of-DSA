#include <stdio.h>

int main(){
int r,c;
scanf("%d %d", &r, &c);

int arr1[r][c];
for(int i=0; i<r; i++){
    for(int j=0; j<c; j++){
        scanf("%d", &arr1[i][j]);
    }
}

int arr2[r][c];
for(int i=0; i<r; i++){
    for(int j=0; j<c; j++){
        scanf("%d", &arr2[i][j]);
    }
}

int sum[r][c];
for(int i=0; i<r; i++){
    for(int j=0; j<c; j++){
        sum[i][j] = arr1[i][j]+arr2[i][j];
    }
}

for(int i=0; i<r; i++){
    for(int j=0; j<c; j++){
        printf("%d ", sum[i][j]);
    }
    printf("\n");
}

return 0;
}
